import subprocess
p = subprocess.Popen(["echo", "hello world"], stdout=subprocess.PIPE)
p
p.communicate()
p = subprocess.Popen(["python", "bmpsim.py"], stdout=subprocess.PIPE)
p
p.communicate()
p = subprocess.Popen(["python", "bmpsim.py -t 30"], stdout=subprocess.PIPE)
p = subprocess.Popen(["python", "bmpsim.py"," -t 30"], stdout=subprocess.PIPE)
